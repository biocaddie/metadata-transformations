  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Subjects with a definitive diagnosis of AD, stringently diagnosed by ADVN Standard Diagnostic Criteria who fall into one of the following categories: Presence of recurrent clinically manifested HSV infection with EH, Presence of recurrent clinically manifested HSV infection without EH, No recurrent clinically manifested HSV infection or EH infection or, Normal control subjects with no personal family history of food allergy, AD, asthma, or allergic rhino-conjunctivitis."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Persons born in or residing in Germany."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Healthy individuals with no systemic disorders other than atopic diseases as outlined in the exclusion criteria."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Individuals between 18-60 years old, inclusive."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Caucasians."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects with atopy but lacking stringent AD features, allowing only a presumptive diagnosis of AD."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Individuals under 18 or over 60 years of age."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects receiving systemic immunosuppressives or chemotherapy within 30 days."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects receiving oral corticosteroids (including inhaled agents) antibiotics, antivirals, anti-inflammatory biologics (e.g., alfacept, etanercept, etc.), topical doxepin, topical coal tar preparations or topical phosphodiesterase inhibitors within 14 days."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects receiving immunotherapy."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects using topical immunotherapy."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects using topical corticosteroids, antibiotics, antivirals, immune enhancers (e.g., imiquimod), or calcineurin inhibitors within 7 days of enrollment."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects receiving phototherapy (e.g., ultraviolet light B [UVB], psoralen plus ultraviolet light A [PUVA]) within 30 days of enrollment."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects having malignancy, autoimmune, or immunodeficiency diseases."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects with the presence of active fungal, bacterial, or viral infections at baseline."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects with the presence of any skin diseases other than AD that might compromise the stratum corneum barrier (e.g., ichthyosis, bullous disease, psoriasis, skin cancer)."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Individuals with mental illness or a history of drug or alcohol abuse that, in the opinion of the investigator, would interfere with the subject's ability to comply with study requirements."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Inability or unwillingness of a subject to give written informed consent."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Pregnant women and breastfeeding mothers."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects weighing <40kg."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects using anxiolytic agents, and anti-depressants."
          ]
        }
